{
  "name": "social-marketing",
  "version": "1.0.11",
  "description": "A module using puppeteer to do social marketing",
  "author": "Robert Zeng",
  "main": ".vite/build/background.js",
  "scripts": {
    "serve": "vite preview",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer && npm run build:task && electron-builder",
    "build:main": "vite build --config vite.main.config.mjs",
    "build:preload": "vite build --config vite.preload.config.mjs",
    "build:renderer": "vite build --config vite.render.config.mjs",
    "build:task": "vite build --config vite.taskCode.config.mjs",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "vite --config vite.main.config.mjs",
    "dev:renderer": "vite --config vite.render.config.mjs",
    "lint": "vue-cli-service lint",
    "builds": "tsc --build",
    "clean": "tsc --build --clean",
    "init": "ts-node src/runcli.ts -a sqlinit",
    "login": "ts-node src/runcli.ts -a login -c",
    "rebuild-better-sqlite": "electron-rebuild -f -w better-sqlite3",
    "prepackage": "npm run rebuild-sqlite3",
    "rebuild-sqlite3": "electron-rebuild -f -w sqlite3"
  },
